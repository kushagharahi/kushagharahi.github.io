{
  "name": "kusha.me",
  "version": "3.0.0",
  "description": "## Instructions * Clone repo * Make sure you have npm installed * do npm install",
  "main": "index.js",
  "scripts": {
    "unix-prod-build": "rm -rf dist/* && webpack --config ./build/webpack.prod.config.js",
    "win-prod-build": "npm run win-clean-dist && webpack --config ./build/webpack.prod.config.js",
    "unix-dev-build": "rm -rf dist/* && webpack --config ./build/webpack.dev.config.js --display-entrypoints",
    "win-dev-build": "npm run win-clean-dist && webpack --config ./build/webpack.dev.config.js --display-entrypoints",
    "unix-start-dev-server": "rm -rf dist/* && webpack-dev-server --config ./build/webpack.dev.config.js",
    "win-start-dev-server": "npm run win-clean-dist && webpack-dev-server --config ./build/webpack.dev.config.js",
    "win-clean-dist": "(rd dist /s /q || echo dist not found, creating) && mkdir dist"
  },
  "repository": "git+https://github.com/kushagharahi/kushagharahi.github.io.git",
  "author": "Kusha Gharahi",
  "license": "MIT",
  "bugs": {
