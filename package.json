    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "2.0.0",
    "html-loader": "^0.5.5",
    "markdown-loader": "^5.1.0",
    "node-sass": "^4.13.1",
    "prerender-spa-plugin": "3.4.0",
    "sass-loader": "7.1.0",
    "sitemap-webpack-plugin": "0.6.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "^2.3.4",
    "vue-loader": "15.4.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "4.26.0",
    "webpack-cli": "3.1.2",
